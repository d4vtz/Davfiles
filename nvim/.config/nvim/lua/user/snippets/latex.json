{
  "Parenthesis": {
      "prefix": "lp",
      "body": [
          "\\left( $1 \\right) $0"
      ],
      "description": "Large parenthesis"
  },
  "Brackets": {
      "prefix": "lb",
      "body": [
          "\\left[ $1 \\right] $0"
      ],
      "description": "Large brackets"
  },
  "Keys": {
      "prefix": "lk",
      "body": [
          "\\left{ $1 \\right} $0"
      ],
      "description": "Large keys"
  },
  "Diff": {
    "prefix": "diff",
    "body": [
      "\\diff{$1}{$2} $0"
    ],
    "description": "Derivate"
  },
  "Diffs": {
    "prefix": "diffs",
    "body": [
      "\\diff[$3]{$1}{$2} $0"
    ],
    "description": "Derivate Orden Superior"
  },
  "Eqref": {
    "prefix": "eqr",
    "body": [
      "\\eqref{$1} $0"
    ]
  },
  "Subindex": {
    "prefix": "subi",
    "body": [
      "_{$1} $0"
    ]
  },
  "Superindex": {
    "prefix": "supi",
    "body": [
      "^{ $1 } $0"
    ]
  }
}
